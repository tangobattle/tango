{
  "name": "tango",
  "productName": "Tango",
  "version": "3.2.58",
  "author": {
    "name": "The Tango Authors",
    "email": "lets@tangobattle.com"
  },
  "private": true,
  "description": "rollback netplay for mega man battle network",
  "main": "./dist/main.js",
  "repository": "https://github.com/tangobattle/tango",
  "scripts": {
    "build:win32-x64": "npm-run-all build:main build:renderer && ./update-win32-x64-bin.sh",
    "build:mac-x64": "npm-run-all build:main build:renderer && ./update-mac-x64-bin.sh",
    "build:linux-x64": "npm-run-all build:main build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack/main.webpack.ts --mode=production",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack/renderer.webpack.ts --mode=production",
    "generate:protos": "(rm -rf src/protos/generated && mkdir -p src/protos/generated 2> /dev/null || true) && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/protos/generated --ts_proto_opt=esModuleInterop=true -I../core/tango-core/src/protos/ ipc.proto && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/protos/generated --ts_proto_opt=esModuleInterop=true -I./src/protos lobby.proto && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/protos/generated --ts_proto_opt=esModuleInterop=true -I../core/tango-protos/src/protos/ iceconfig.proto replay.proto",
    "start": "npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register/transpile-only ./src/main/main.ts",
    "start:renderer": "cross-env NODE_ENV=development webpack serve --config webpack/renderer.webpack.ts --mode=development",
    "dist:win32-x64": "npm run build:win32-x64 && electron-builder --win --x64",
    "dist:mac-x64": "npm run build:mac-x64 && electron-builder --mac --x64",
    "dist:linux-x64": "npm run build:linux-x64 && electron-builder --linux --x64"
  },
  "keywords": [],
  "license": "AGPL-3.0",
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.5.3",
    "async-mutex": "^0.3.2",
    "crc-32": "^1.2.2",
    "css-loader": "^6.7.1",
    "date-fns": "^2.28.0",
    "discord-rpc": "^4.0.1",
    "electron-log": "^4.4.6",
    "electron-updater": "^5.0.1",
    "email-addresses": "^5.0.0",
    "event-iterator": "^2.0.0",
    "i18next-browser-languagedetector": "^6.1.4",
    "isomorphic-git": "^1.17.2",
    "json5-loader": "^4.0.1",
    "lodash-es": "^4.17.21",
    "long": "^5.2.0",
    "mkdirp": "^1.0.4",
    "node-fetch": "^3.2.4",
    "protobufjs": "^6.11.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.7",
    "react-sparklines": "^1.7.0",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.3.2",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "semver": "^7.3.7",
    "sha3": "^2.1.4",
    "style-loader": "^3.3.1",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@types/discord-rpc": "^4.0.2",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/lodash-es": "^4.17.6",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-sparklines": "^1.7.2",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/semver": "^7.3.9",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "cross-env": "^7.0.3",
    "electron": "^18.2.0",
    "electron-builder": "^23.1.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "ts-proto": "^1.112.0",
    "typescript": "^4.6.3",
    "wait-on": "^6.0.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "build": {
    "appId": "com.tangobattle.tango",
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "category": "public.app-category.games"
    },
    "directories": {
      "output": "packages"
    },
    "files": [
      "package.json",
      "dist/**"
    ],
    "extraResources": [
      "bin/**"
    ]
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.13.0"
  }
}
